<% provide(:title, 'ユーザ一覧') %>
<h1>ユーザ一覧</h1>
<%= will_paginate %>

<%= form_tag({controller: "users", action: "import", method: "post"}, {multipart: true}) do %>
  <%= file_field_tag :file %>
  <%= submit_tag "csvをインポート" ,class: "btn btn-sm btn-primary" %>
<% end %>

<ul class="users">
  <%= render @users %>
</ul>
